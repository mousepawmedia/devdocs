{
    // Disable RST linter
    "restructuredtext.linter.run": "off",
    "restructuredtext.confPath": "${workspaceRoot}/network/source",
    "restructuredtext.workspaceRoot": "/home/jason/Code/Repositories/devdocs/network",
    "restructuredtext.builtDocumentationPath": "${workspaceRoot}/network/build/html",

    "cSpell.ignorePaths": [
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        ".vscode",
        "typings"
      ],

      "cSpell.ignoreWords": [
          "codemouse",
          "importrepos",
          "ldunn",
          "newbranchname",
          "paren",
          "phunyguy",
          "wontfix",
          "yourbranchname",
      ],

    "cSpell.ignoreRegExpList": [
        // Skip hex values
        "0x[a-f0-9]+",
        // Skip reference labels
        "\\.\\. +_\\w+:",
        // Skip :ref: directives
        ":ref:`.+`",
        // Skip bash commands
        "    \\$.*"
    ],

