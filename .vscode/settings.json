    "cSpell.ignoreRegExpList": [
        // Skip hex values
        "0x[a-f0-9]+",
        // Skip reference labels
        "\\.\\. +_\\w+:",
        // Skip :ref: directives
        ":ref:`.+`",
        // Skip bash commands
        "    \\$.*",
        // Skip C++ include directives
        "#include .+",
        // Skip :file: directives
        ":file:`.+`",
        // Skip code filenames
        "`\\w+\\.(h|hpp|c|cpp|py|conf)`",
        // Skip file extensions
        "``\\*\\.\\w+``",
        // Skip URLs
        "http(s):\\/\\/(\\.\\-\\w\\/)+"
    ],

